


class parent
{
	
	int a;
	void dis()
	{
		System.out.println(a);
	}
	
}

class child1 extends parent
{
	int b=200;
	void sh()
	{
		System.out.println(b);
	}
}

class child2 extends parent
{
	int c=300;
	void bizz()
	{
		System.out.println(c);
	}
}
public class child {

	public static void main(String[] args) 
	{
		child1 obj =new child1 ();
		obj.a=100;
		obj.dis();
		obj.sh();
		child2 obj1 =new child2();
		obj1.a=900;
		obj1.dis();
		obj1.bizz();
		
	}

}
